@model IEnumerable<AccessModels.Models.Homework>

@{
    ViewBag.Title = "GetOneTeacherHomework";
}

<h2>GetOneTeacherHomework</h2>

<p>
    @Html.ActionLink("Create New", "CreateHomework","Teacher")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HomeworkName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeacherUserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Deadline)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HomeworkName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TeacherUserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deadline)
        </td>
        
    </tr>
}

</table>
